{
  "name": "atomNodeAPI",
  "version": "1.3.12",
  "private": true,
  "scripts": {
    "localhost-test": "NODE_ENV=test PORT=8888 nodemon ./app.coffee",
    "localhost-dev": "NODE_ENV=development PORT=8888 nodemon ./app.coffee",
    "localhost-prod": "NODE_ENV=production PORT=8888 nodemon ./app.coffee",
    "start": "NODE_ENV=development PORT=8888 nodemon ./app.coffee",
    "dev": "NODE_ENV=development PORT=8888 DEBUG=atomNodeAPI forever start -c coffee ./app.coffee",
    "prod": "NODE_ENV=production PORT=8888 DEBUG=atomNodeAPI pm2 start -n management ./start.js",
    "backup": "NODE_ENV=backup PORT=8888 DEBUG=atomNodeAPI pm2 start -n management ./start.js",
    "test": "mocha --compilers coffee:coffee-script/register",
    "web-log": "pm2 web",
    "web": "pm2-web"
  },
  "dependencies": {
    "async": "^0.9.0",
    "blueimp-md5": "^1.1.0",
    "body-parser": "~1.8.1",
    "coffee-script": "^1.8.0",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "lodash": "^2.4.1",
    "excel-export": "^0.4.1",
    "moment": "^2.9.0",
    "mongoose": "^3.8.22",
    "morgan": "~1.3.0",
    "mysql": "^2.5.4",
    "node-xlsx": "^0.5.1",
    "q": "^1.1.2",
    "request": "^2.53.0",
    "require-dir": "^0.1.0",
    "sequelize": "^2.0.1",
    "sequelize-cli": "^0.3.3",
    "underscore": "^1.7.0",
    "xlsx": "^0.7.12"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "newrelic": "^1.14.3",
    "nodemon": "^1.3.7"
  }
}
